[core]
	longpaths = true
[user]
	email = johnqwerty.lee1@gmail.com
	name = John Lee
[branch]
	sort = committerdate
[alias]
	kick = commit --allow-empty -m \"kick pipe\" && git push
	pushf = push --force-with-lease
	pushff = push -f
	unskip = update-index --no-skip-worktree
	skip = update-index --skip-worktree
	prdiffs = "!f() { git diff $(git merge-base master HEAD); }; f"
	prlog = "!f() { git log --oneline $(git merge-base master HEAD)..HEAD; }; f"
	pradds = "!f() { git diff --name-only --diff-filter=A $(git merge-base master HEAD); }; f"
	prmods = "!f() { git diff --name-only --diff-filter=M $(git merge-base master HEAD); }; f"
[push]
	autoSetupRemote = true
[http]
	postbuffer = 52428800
[rebase]
	autoStash = true
[safe]
	directory = /storage/emulated/0/notes/second-brain
	directory = /storage/emulated/0/notes/books
	directory = /storage/emulated/0/notes/music-sheets
	directory = /storage/emulated/0/notes/vansoftworks-technical-wiki
        directory = /storage/emulated/0/notes/notes-archive
	directory = /storage/emulated/0/notes/rubiks-cube-notes
[fetch]
	writeCommitGraph = true
